<odoo>

    <record id="view_quant_tree_inherit_editable" model="ir.ui.view">
        <field name="name">stock.quant.tree.inherit.editable</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field name="equipment_id"/>
            </field>
        </field>
    </record>



    <record id="view_quant_tree_inherit" model="ir.ui.view">
        <field name="name">stock.quant.tree.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field name="equipment_id"/>
            </field>
        </field>
    </record>

    <record id="view_quant_form_inherit" model="ir.ui.view">
        <field name="name">stock.quant.form.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_form"/>
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field name="equipment_id"/>
            </field>
        </field>
    </record>
</odoo>
